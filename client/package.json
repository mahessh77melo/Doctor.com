{
	"name": "doctor",
	"version": "1.0.0",
	"description": "Landing Page for Doctor.com project",
	"main": "index.js",
	"scripts": {
		"watch:sass": "node-sass static/scss/base.scss static/css/doc_style.css -w",
		"compile:sass": "node-sass static/scss/base.scss static/css/doc_style.css",
		"devserver": "live-server",
		"start": "npm-run-all --parallel watch:sass devserver",
		"prefix:sass": "postcss --use autoprefixer -b \"last 10 versions\" static/css/doc_style.css -o static/css/style.prefix.css",
		"compress:sass": "node-sass static/css/style.prefix.css static/css/style.css --output-style compressed",
		"build:css": "npm-run-all compile:sass prefix:sass compress:sass"
	},
	"author": "Magesh",
	"license": "ISC",
	"devDependencies": {
		"autoprefixer": "^9.8.6",
		"node-sass": "^4.14.1",
		"postcss-cli": "^7.1.1"
	},
	"dependencies": {
		"material-design-icons": "^3.0.1",
		"node-fetch": "^2.6.1",
		"npm-run-all": "^4.1.5"
	}
}
